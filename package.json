{
  "name": "taro-template",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "dev": "cross-env REACT_APP_ENV=dev npm run build:weapp -- --watch",
    "test": "cross-env REACT_APP_ENV=test npm run build:weapp -- --watch",
    "pre": "cross-env REACT_APP_ENV=pre npm run build:weapp -- --watch",
    "prod": "cross-env REACT_APP_ENV=prod npm run build:weapp -- --watch",
    "build": "cross-env REACT_APP_ENV=prod taro build --type weapp",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postinstall": "husky install",
    "format": "prettier --write",
    "lint": "yarn run lint:script",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:es/css": "npm run lint:es && npm run lint:css",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@commitlint/config-conventional": "^17.6.3",
    "@tarojs/components": "3.5.4",
    "@tarojs/helper": "3.5.4",
    "@tarojs/plugin-framework-react": "3.5.4",
    "@tarojs/plugin-platform-alipay": "3.5.4",
    "@tarojs/plugin-platform-jd": "3.5.4",
    "@tarojs/plugin-platform-qq": "3.5.4",
    "@tarojs/plugin-platform-swan": "3.5.4",
    "@tarojs/plugin-platform-tt": "3.5.4",
    "@tarojs/plugin-platform-weapp": "3.5.4",
    "@tarojs/react": "3.5.4",
    "@tarojs/router": "3.5.4",
    "@tarojs/runtime": "3.5.4",
    "@tarojs/shared": "3.5.4",
    "@tarojs/taro": "3.5.4",
    "@tarojs/taro-h5": "3.5.4",
    "base-64": "^1.0.0",
    "classnames": "^2.3.1",
    "conventional-changelog-cli": "^2.2.2",
    "core-decorators": "^0.20.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.5",
    "husky": "^8.0.3",
    "i18next": "^21.10.0",
    "jest": "^29.3.1",
    "jotai": "^2.1.0",
    "lint-staged": "^13.2.2",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "prerender-spa-plugin": "git+https://github.com/Eimi-Fukada/Prerender-spa-plugin.git",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "taro-ui": "3.0.0-alpha.10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "3.5.4",
    "@tarojs/webpack5-runner": "3.5.4",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-preset-taro": "3.5.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-config-taro": "3.5.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react-refresh": "^0.11.0",
    "stylelint": "^15.6.1",
    "typescript": "^4.1.0",
    "webpack": "5.69.0"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
