{
  "name": "taro-template",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "dev": "cross-env REACT_APP_ENV=dev npm run build:h5 -- --watch",
    "test": "cross-env REACT_APP_ENV=test npm run build:weapp -- --watch",
    "pre": "cross-env REACT_APP_ENV=pre npm run build:weapp -- --watch",
    "prod": "cross-env REACT_APP_ENV=prod npm run build:weapp -- --watch",
    "build": "cross-env REACT_APP_ENV=prod taro build --type weapp",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "format": "prettier --write",
    "lint": "npm run lint:script",
    "lint:css": "stylelint \"src/**/*.{css,less}\" --no-ignore",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:es/css": "npm run lint:es && npm run lint:css",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "postinstall": "weapp-tw patch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@commitlint/config-conventional": "^17.6.3",
    "@nutui/nutui-react-taro": "^3.0.18-cpp.beta.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tarojs/components": "4.1.5",
    "@tarojs/helper": "4.1.5",
    "@tarojs/plugin-framework-react": "4.1.5",
    "@tarojs/plugin-html": "4.1.5",
    "@tarojs/plugin-platform-alipay": "4.1.5",
    "@tarojs/plugin-platform-h5": "4.1.5",
    "@tarojs/plugin-platform-jd": "4.1.5",
    "@tarojs/plugin-platform-qq": "4.1.5",
    "@tarojs/plugin-platform-swan": "4.1.5",
    "@tarojs/plugin-platform-tt": "4.1.5",
    "@tarojs/plugin-platform-weapp": "4.1.5",
    "@tarojs/react": "4.1.5",
    "@tarojs/router": "4.1.5",
    "@tarojs/runtime": "4.1.5",
    "@tarojs/shared": "4.1.5",
    "@tarojs/taro": "4.1.5",
    "@tarojs/taro-h5": "4.1.5",
    "babel-plugin-import": "^1.13.8",
    "base-64": "^1.0.0",
    "classnames": "^2.5.1",
    "conventional-changelog-cli": "^2.2.2",
    "core-decorators": "^0.20.0",
    "cross-env": "^10.0.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "husky": "^9.1.7",
    "i18next": "^25.3.4",
    "jest": "^30.0.5",
    "jotai": "^2.1.0",
    "lint-staged": "^13.2.2",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "postcss-rem-to-responsive-pixel": "^6.0.2",
    "prerender-spa-plugin": "git+https://github.com/Eimi-Fukada/Prerender-spa-plugin.git",
    "prettier": "^3.6.2",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "taro-ui": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-react": "^7.27.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "4.1.5",
    "@tarojs/webpack5-runner": "4.1.5",
    "@types/react": "^18.3.1",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.4.21",
    "babel-preset-taro": "4.1.5",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-config-taro": "4.1.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.6",
    "react-refresh": "^0.17.0",
    "stylelint": "^15.6.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "weapp-tailwindcss": "^4.2.5",
    "webpack": "^5.91.0"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
